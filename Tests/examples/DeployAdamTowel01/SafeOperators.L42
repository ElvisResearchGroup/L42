{'@plugin
  'L42.is/connected/withSafeOperators

  type method
  Library compose(Library left,Library right)
    using Outer0
      check compose(left:left,right:right)
      error Report::PluginFailure"SafeOperators.compose"

  type method
  Library renameClass(Library that,Path src, Path dest)
    using Outer0
      check renameClass(that
        src:src.binaryRepr()
        dest:dest.binaryRepr()
        )
      error Report::PluginFailure"SafeOperators.renameClass"

  type method
  Library renameMethod(Library that,Path path,Selector src,Selector dest)
    using Outer0
      check renameMethod(that
        path:path.binaryRepr()
        src:src.binaryRepr()
        dest:dest.binaryRepr()
        )
      error Report::PluginFailure"SafeOperators.renameMethod"

  type method
  Library redirect(Library that,Path src, type Any dest)
    using Outer0
      check redirect(that
        src:src.binaryRepr()
        dest:dest
        )
      error Report::PluginFailure"SafeOperators.redirect"

  type method
  Library removeImplementation(Library that,Path path)
    using Outer0
      check removeImplementation(that
        path:path.binaryRepr()
        )
      error Report::PluginFailure"SafeOperators.removeImplementation(path)"

  type method
  Library removeImplementation(Library that,Path path,Selector selector)
    using Outer0
      check removeImplementation(that
        path:path.binaryRepr()
        selector:selector.binaryRepr()
        )
      error Report::PluginFailure"SafeOperators.removeImplementation(path,selector)"

  type method
  Library addDocumentation(Library that,Path path,Doc doc)
    using Outer0
      check addDocumentation(that
        path:path.binaryRepr()
        doc:doc.binaryRepr()
        )
      error Report::PluginFailure"SafeOperators.addDocumentation(path)"

  type method
  Library addDocumentation(Library that,Path path,Selector selector,Doc doc)
    using Outer0
      check addDocumentation(that
        path:path.binaryRepr()
        selector:selector.binaryRepr()
        doc:doc.binaryRepr()
        )
      error Report::PluginFailure"SafeOperators.addDocumentation(path,selector)"

  }
