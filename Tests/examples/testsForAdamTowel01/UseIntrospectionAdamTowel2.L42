{reuse L42.is/AdamTowel01
External:{}
Main:{
  i2=Introspection(lib:{ Internal:{} method 'fuffa @a beer @External bar @Internal fuzz
    Void foo()})
  mFoo=i2.methodFromSelector(Selector"foo()")
  doc=mFoo.doc()
  Debug(doc)
  with ann in doc.annotations().vals() (
    Debug(ann)
    (
      with t=ann.referredClass() (
        on type External Debug(S"External found")
        default Debug(S"something Else")
        )
      catch error x (
        on Report::PluginFailure Debug(S"Report plgFailure as "++x.toS())
        on Report Debug(S"Report error as "++x.toS())
        )
      void
      )
    Debug(S"Iteration complete")
    )
  return ExitCode.normal()
  }
}