{reuse L42.is/AdamTowel01

Main:{

  i=Introspection(lib:{Bar:{method Void foo(Void par1 Any par2)} Beer:{}},path:Path"Bar")
  Debug(S"classkind of Bar is "++i.classKind())
  Debug(S"Bas as string: "++i.allAsString())
  Debug(S"--------------------------")
  Debug(S"classkind of Outer0 is "++i.root().classKind())
  Debug(S"Outer0 as string: "++i.root().allAsString())
  Debug(S"--------------------------")
  Debug(S"for all the methods of Bar:")
  ms=i.methods()
  with m in ms.vals() (
   Debug(S"selector is: "[m.selector()]"")
   ti=m.returnType()
   Debug(S"return type is:"[ti]"")
   with parT in m.parameterTypes().vals() (
     Debug(S"parameter type is:"[parT]"")  
     )
   'ti.record().dbg()
   )
  Debug(S"--------------------------")
  Debug(S"for all the nested classes of Bar:")
  with nc in i.root().nestedClasses().vals() (
    Debug(S"Nested class path is:"[nc.pathFromRoot()]"")
    Debug(nc)
    )
  return ExitCode.normal()
  }
}