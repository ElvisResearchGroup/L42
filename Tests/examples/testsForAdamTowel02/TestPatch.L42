{reuse L42.is/AdamTowel02

C:Patch(Path"P")<<{
  A:{type method Void foo()Debug(S"World")}
  P:{
    A:{type method Void foo() (Debug(S"Hello") this.#1foo() )}
    B:{type method Void foo() (Debug(S"Hello-") A.#1foo() )}
    }
  }
D:Patch(Path"P")<<{
  A:{
    type method A foo(A that)this.foo(that)
    type method B bar(B that)this.bar(that)
    }
  B:{}
  P:{
    A:{
      type method A foo(A that)that
      type method B bar(B that)that
      }       
    B:{type method B bar(B that)that} 
    }
  }
Main:{
  C::A.foo()
  C::B.foo()
  Debug(S"OK")
  return ExitCode.normal()
  }
}

