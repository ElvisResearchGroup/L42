Vector pos
Vector speed
read method read Map.Entity.Opt target(read Map.Entity.List that)
mut method Void moveSpeed(Double rad,Size power)
mut method Void chase(mut Map.Env that)=(
  selfP=this.pos()
  target=this.target(that.es())
  targetP=if target (target.val().pos())
    else (this.pos()+this.speed())
  x=targetP.x()-selfP.x()
  y=targetP.y()-selfP.y()
  this.moveSpeed(rad=x.atan2(y),power=1\)
  )