[Map.Entity]
class method mut This()=
  This(pos=Vector(x=35\,y=100\),speed=Vector(x=0\,y=0\),text=S"",
    left=Bool.false(),right=Bool.false(),up=Bool.false(),down=Bool.false())
var S text
var Vector pos
var Vector speed
read method Bool solid()=\.true()
read method Double radius()=20\
read method Double break()=\"-0.15"
read method Double topSpeed()=5\
read method Double acc()=\"0.08"
read method S img()=S"https://www.google.com/images/branding/googlelogo/2x/googlelogo_color_92x30dp.png"
mut method Void keyMove()
method ping(that)=(
  this.keyMove()
  this.moveImpact(that)
  )
mut method Void playerIsHit()=(
    this.pos(\(x=200\,y=200\)) 
    this.speed(\(x=0\,y=0\))
    )    

mut method Bool onImpact(mut Map.Env that,Vector oldP, mut Map.Entity impacted)={
  if impacted<:mut Rat (
    this.playerIsHit()
    return \.true()
    )
  return \.false()
  }