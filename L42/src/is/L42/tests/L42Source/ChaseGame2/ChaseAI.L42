Vector pos
Vector speed
mut method mut $.Entity.Opt target()
mut method Void moveSpeed(Double rad,Size power)
mut method Void chase()=(
  selfP=this.pos()
  target=this.target()
  targetP=if target (target.val().pos())
    else (this.pos()+this.speed())
  x=targetP.x()-selfP.x()
  y=targetP.y()-selfP.y()
  this.moveSpeed(rad=x.atan2(y),power=1\)
  )