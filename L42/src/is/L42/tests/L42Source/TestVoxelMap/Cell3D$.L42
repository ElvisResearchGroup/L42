I x, I y, I z

class method Cell3D random(mut Math.Random r) = \(
  x=r.nextInRange(start=0\ end=Margin.maxX())
  y=r.nextInRange(start=0\ end=Margin.maxY())
  z=r.nextInRange(start=0\ end=Margin.maxZ())
  )
method Point3D toPoint() = \(
  x=this.x().toDouble()
  y=this.y().toDouble()
  z=this.z().toDouble()
  )
method Cell2D to2D() = \(x=this.x(), y=this.y())

method Bool isBorder() = (
  (x, y, z) = this
  x==0I || x==Margin.maxX()-1I
    || y==0I || y==Margin.maxY()-1I
    || z==0I || z==Margin.maxZ()-1I
  )