Size x
Size y

method Cell2D random(mut Math.Random r)=\(
  x=r.nextInRange(start=0\ end=Margin.maxX())
  y=r.nextInRange(start=0\ end=Margin.maxY())
  )

method Cell2D$List adjacent8()=(
  (x,y) = this
  \[
    \(x=x-1\, y=y-1\);\(x=x-1\, y=y);\(x=x-1\, y=y+1\);
    \(x=x,    y=y-1\);               \(x=x,    y=y+1\);
    \(x=x+1\, y=y-1\);\(x=x+1\, y=y);\(x=x+1\, y=y+1\);
    ]
  )
method Cell2D$List adjacent4()=(
  (x,y) = this
  \[
                      \(x=x-1\, y=y);
    \(x=x,    y=y-1\);               \(x=x,    y=y+1\);
                      \(x=x+1\, y=y);
    ]
  )

method Point2D toPoint() = 
  \(x=this.x().toDouble(), y=this.y().toDouble())
method Cell3D to3D(Size z) =
  \(x=this.x(), y=this.y(), z=z)