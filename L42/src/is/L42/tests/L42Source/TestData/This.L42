reuse[AdamTowel]

TestDataBase={
  Point=Data:{Size x, Size y}
  TestPoint=(
    p1=Point(x=8\, y=42\)
    p2=Point(x=8\, y=42\)
    p3=Point(x=0\, y=0\)
    {}:Test"pointEq1"(p1==p2)
    {}:Test"pointEq2"(p1!=p3)
    {}:Test"pointEqWithy"(p1!=p2.with(y=0\))
    {}:Test"pointEqWithxy"(p1==p3.with(y=p1.y()).with(x=p1.x()))
    )
  }

   