reuse [AdamTowel]
VMT = Load : {reuse [VoxelMapTrait] }
V = Class: VMT : {
  class method Size maxX()=64\
  class method Size maxY()=64\
  class method Size maxZ()=100\
  }
SizeMap = Class : Data : V.map2D(Size.List) : {
  class method Size base() = 0Size
  }
BoolMap = Class : Data : V.map2D(Bool.List) : {
  class method Bool base() = Bool.false()
  }
ShuffleSize = Math.randomList(Size.List)
ShuffleCell = Math.randomList(V.Cell2D.List)
CellularExpansion = {...}
BoolExpansion = Class: Data: CellularExpansion(BoolMap): {
  read method Bool combine(V.Cell2D that, Bool seed) = seed
  }
HeightExpansion = Class: Data: CellularExpansion(SizeMap): {...}

Foo=(
