reuse [AdamTowel]
Main=(
  Debug(S"Hello world")
  )
Foo={
  @Cache.Call class method Void foo()=(
    Debug(S"foo called")
    Log"Foo".write(S"foo called")
    )
  }
MkTest=Organize:{
  class method Void callFoo()=Foo.foo()
  }
TestCacheCall=(
  Log"Foo".clear()
  MkTest.callFoo()
  MkTest.callFoo()
  MkTest.callFoo()
  {}:Test""(actual=Log"Foo".#$reader().read()
    expected=S"foo called"
    )
  )