class method Void #$of() = (
  r1=Math.Random.#$random()
  r2=Math.Random.#$random()
  imm s1=ResultSigner.#$of(id=S"1")
  imm s2=ResultSigner.#$of(id=S"2")
  @Player1 Board b2=Players.AIPlayer1.allShips(rand=r1)
  @Player2 Board b1=Players.AIPlayer2.allShips(rand=r2)
  p1=Players.AIPlayer1(board=b1,rand=r1,signer=s1,declassify=DeclassifyP1.#$of(),myRound=Bool.true())
  p2=Players.AIPlayer2(board=b2,rand=r2,signer=s2,declassify=DeclassifyP2.#$of(),myRound=Bool.false())
  Debug(b1)
  Debug(b2)
  res=this.playGame(p1=p1,p2=p2)
  Debug(p1.myShots())
  Debug(p2.myShots())
  Debug(res)
  )
class method S playGame(mut Players.AIPlayer1 p1, mut Players.AIPlayer2 p2) = {
  var rounds = 0I
  loop (
    rounds += 1I
    winP1 = p1.round(other=p2)
    if winP1 && p2.agreeToLose()
      return S"Player1 wins the game after %rounds rounds"
    winP2 = p2.round(other=p1)
    if winP2 && p1.agreeToLose()
      return S"Player2 wins the game after %rounds rounds"
    )
  }