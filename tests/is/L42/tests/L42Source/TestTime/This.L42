reuse [AdamsTowel]

Secret = {...}//THIS FILE IS NOT COMMITTED. You have to fill it with you git access token, as in
//class method S #$of()=S"ghp_..."

GW = Load:{reuse[L42.is/GitWriter]}
LoadDeploy = Load:{reuse[L42.is/Deploy]}
DeployGit = LoadDeploy.with(writer=GW)
TimeLib = Trait:{...}
TaskDeploy = DeployGit.module(TimeLib['Time$Real=>'Time.Real]
  name='Time
  on=Url"github.com/%Secret.repo()/%Secret.version()/Time.L42",
  writer=GW.#$of(
    token=Secret.#$of(),
    message=S"Time automatic release 42V"++Secret.version()
  ))

Time = Load:{reuse[#$L42.is/Time]}
Main = (
  t = Time.Real.#$of()
  s = t.dateTime(zoneId=S"Europe/Rome",pattern=S"yyyy/MM/dd HH:mm:ss OOOO")
  Debug(S"[[%s]]")
  {}:Test"sExists"(s!=S"")
  long = t.currentTime()
  {}:Test"timeNot70"(long>70Math.Long)
  )