reuse [AdamTowel]
Main=(
  Debug(S"Hello world")
  )

Base=Trait:{reuse [AdamTowel]
  Sifo=Public:Organize:{...}
  }
  
DeployUnit=Deploy.module(
  url='Sifo
  code=Base.code()
  name='Sifo
  )

Sifo=Load:{reuse[#$Sifo]}

SifoTest={
  Left=Sifo:{}
  Right=Sifo:{}
  Top=Sifo:{[Left,Right]}
  DeclassifyS=Sifo.declassify(S top=Top)
  DeclassifySCapsule=Sifo.declassifyCapsule(S top=Top)
  Code=Sifo(Top):{...}
  Main=(
    _=Log(S"hi").#$reader()
    Code.main()
    {}:Test"sifo passed"(Bool.true())
    )
  }